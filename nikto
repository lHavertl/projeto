echo -e '\033[1;36m mbe - \033[1;33m " log to metasploit " \033[0m '
echo -e '\033[1;36m txt - \033[1;33m " plain text " \033[0m '
echo -e '\033[1;36m xml - \033[1;33m " xml format " \033[0m '
echo ""
