case $pro in
127.0.0.1:*) echo $pro2;;
esac
}

func2(){
case $disp in
1)echo 1;;
2)echo 2;;
3)echo 3;;
4)echo 4;;
D)echo D;;
E)echo E;;
P)echo P;;
S)echo S;;
V)echo V;;
*)exit;;
esac
}

func3(){
case $turn in
1)echo 1;;
2)echo 2;;
3)echo 3;;
4)echo 4;;
5)echo 5;;
6)echo 6;;
7)echo 7;;
8)echo 8;;
9)echo 9;;
a)echo a;;
b)echo b;;
c)echo c;;
x)echo x;;
*)exit;;
esac
}

func4(){
case $form in
csr)echo csr;;
htm)echo htm;;
mbe)echo mbe;;
txt)echo txt;;
xml)echo xml;;
*)exit;;
esac
}

func5(){
case $inc1 in
iniciar)echo 'iniciar';;
acrescentar)
var5=acres
read -p "Acrescenta aqui o comando e argumento:   " acres
;;
esac
}

func6(){
case $inc2 in
iniciar)echo 'iniciar';;
acrescentar)
var5=acres
read -p "Acrescenta aqui o comando e argumento:   " acres
;;
esac
}

f1=`func1`
f2=`func2`
f3=`func3`
f4=`func4`
f5=`func5`
f6=`func6`

if [[ $url ]] && [[ $port ]] && [[ $f1 ]] && [[ $f2 ]] && [[ $f3 ]] && [[ $f4 ]]
then
nikto -h $url -port $port -useproxy $pro -Display $disp -Tuning $turn -o $form $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f1 ]] && [[ $f2 ]] && [[ $f4 ]]
then
ini3=nikto -h $url -port $port -useproxy $pro -Display $disp -o $form $acres 
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f1 ]] && [[ $f4 ]]
then
nikto -h $url -port $port -useproxy $pro -o $form $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f2 ]] && [[ $f3 ]] && [[ $f4 ]]
then
nikto -h $url -port $port -Display $disp -Tuning $turn -o $form $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f3 ]] && [[ $f4 ]]
then
nikto -h $url -port $port -Tuning $turn -o $form $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f3 ]]
then
nikto -h $url -port $port -Tuning $turn $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f3 ]] && [[ $f1 ]] && [[ $f4 ]]
then
nikto -h $url -port $port -Tuning $turn -useproxy $pro -o $form $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f3 ]] && [[ $f1 ]]
then
nikto -h $url -port $port -Tuning $turn -useproxy $pro $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $f2 ]] && [[ $f3 ]]
then
nikto -h $url -port $port -Tuning $turn -Display $disp $acres
exit
fi

if [[ $url ]] && [[ $port ]]
then
nikto -h $url -port $port $acres
exit
fi

if [[ $url ]] && [[ $port ]] && [[ $form ]]
then
nikto -h $url -port $port $acres
exit
fi
